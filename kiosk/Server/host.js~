$("document").ready(function(){
$("#web").click(function(){$("#local").attr('checked',false)});
$("#local").click(function(){$("#web").attr('checked',false)});
$("#launch").click(function(){
	var source = $("#source").val();
	var type = $("#local").checked();
	if (type==true) {var type = "local"}
	else {var type = "web"}
});
});

